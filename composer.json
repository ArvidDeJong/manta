{
    "name": "darvis/manta",
    "description": "Een krachtig, out-of-the-box Content Management Systeem voor Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Darvis",
            "email": "jouw-email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11|^12",
        "laravel/framework": "^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "laravel/jetstream": "^5.0",
        "unisharp/laravel-filemanager": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Darvis\\Manta\\": "src/"
        },
        "files": [
            "src/Helpers/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Darvis\\Manta\\MantaServiceProvider"
            ]
        }
    }
}